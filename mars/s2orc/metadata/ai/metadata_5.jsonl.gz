, "211011032", "52170093", "67856057", "173187820", "3602650", "22191393", "44119895", "207758212", "196178008", "204743918", "202539732", "208291464", "209515478", "40144544", "118721501", "51878207", "204823772", "212628373", "209353355", "201651184", "211043697", "4398957", "19158108", "162184501", "53221030", "209379877", "199668725", "203737227", "196471193", "204772060", "52282359", "204728578", "90262387", "85543335", "86798862", "211297847", "214714005", "34175434", "210849166", "53038136", "53301931", "212717769", "210164865", "204743739", "58028844", "210928750", "210710430", "201660501", "53965901", "120397368", "120397368", "3952914", "13749281", "203642006", "53183710", "102351565", "195750841", "53719014", "195419665", "203736522", "52877454", "198179467", "202783263", "67872611", "57573724", "201659797", "211171784", "204904728", "209444713", "210715143", "210715224", "210995672", "201668869", "67856120", "211020745", "202783932", "204782951", "202768445", "19188052", "53426963", "57361315", "135464984", "199405543", "210932292", "211817757", "158046817", "214612335", "214612335", "52343024", "59553503", "3973810", "204915973", "202770936", "215455251", "214693368", "53027350", "210712581", "36089872", "204512528", "166228715", "215416034", "52895589", "214714025", "202895301", "202541059", "211211445", "209316466", "202888963", "195750685", "131777802", "3486251", "210715216", "196182275", "211116822", "49324734", "53782673", "59222740", "195750912", "201070403", "214667366", "196207205", "214802399", "67856641", "50776071", "121292321", "198354047", "3340632", "215428812", "170078608", "53223941", "198179710", "166228294", "214713592", "211266979", "204027863", "152282511", "53367181", "53080190", "57362238", "121100717", "212644722", "53029967", "46954768", "210715144", "201625275", "166228147", "52897567", "145053285", "53305686", "207863686", "53214254", "195755445", "53042693", "52171640", "202596942", "210992070", "52049859", "201103955", "2157