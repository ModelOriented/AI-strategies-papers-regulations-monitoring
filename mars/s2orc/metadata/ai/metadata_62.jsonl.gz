{"paper_id": "15727142", "title": "Multi-Graph Learning with Positive and Unlabeled Bags", "authors": [{"first": "Jia", "middle": [], "last": "Wu", "suffix": ""}, {"first": "Zhibin", "middle": [], "last": "Hong", "suffix": ""}, {"first": "Shirui", "middle": [], "last": "Pan", "suffix": ""}, {"first": "Xingquan", "middle": [], "last": "Zhu", "suffix": ""}, {"first": "Chengqi", "middle": [], "last": "Zhang", "suffix": ""}, {"first": "Zhihua", "middle": [], "last": "Cai", "suffix": ""}], "abstract": null, "year": 2014, "arxiv_id": null, "acl_id": null, "pmc_id": null, "pubmed_id": null, "doi": "10.1137/1.9781611973440.25", "venue": "SDM", "journal": null, "has_pdf_body_text": true, "mag_id": "817531966", "mag_field_of_study": ["Computer Science"], "outbound_citations": ["7398727", "5967441", "16960812", "2033430", "5992648", "371647", "12334122", "14177182", "10705189", "10118968", "14915397", "7664224", "13197749", "3047006", "18196285", "53307800"], "inbound_citations": ["206664102", "21190117", "15861454", "41260758", "207323190", "7418847", "52910113", "16614798", "207323096", "16124236", "14122668", "12347240", "13748961"], "has_outbound_citations": true, "has_inbound_citations": true, "has_pdf_parse": true, "has_pdf_parsed_abstract": true, "has_pdf_parsed_body_text": true, "has_pdf_parsed_bib_entries": true, "has_pdf_parsed_ref_entries": true, "s2_url": "https://api.semanticscholar.org/CorpusID:15727142"}
