 "13328083", "351666", "8298825", "9280155", "38841447", "30229060", "2243454", "211193961", "12342032", "3328765", "15772469", "16381878", "12831960", "11135765", "20374487", "16163331", "16032523", "10664856", "3932678", "9947483", "24199641", "885002", "207852623", "15261634", "59620217", "13188461", "37879188", "1642325", "17998423", "6020574", "11729307", "48356156", "57680", "195878371", "14460763", "17796735", "16630111", "9381712", "17569654", "15488392", "7274230", "26069779", "14338164", "12580643", "338964", "5633594", "35952988", "10419500", "63784973", "1811507", "55575683", "2727312", "7940368", "16249633", "15459403", "6036000", "32487632", "198932285", "8847630", "328583", "189859282", "6910651", "840255", "725277", "832875", "160032559", "8829552", "6083506", "1856687", "5743807", "10991588", "2299809", "5830441", "2075405", "85561426", "2310877", "302156", "2058577", "52108421", "8161857", "6293794", "16801626", "16462827", "2267431", "1618191", "49189287", "16930597", "207608860", "14254554", "4263841", "15348857", "18366554", "15833432", "14685870", "15799398", "1346809", "18433537", "18866747", "7959003", "1457849", "38802920", "2694350", "5393303", "10770509", "7896529", "15488747", "1311568", "12517841", "18472748", "1558784", "8991616", "10995480", "5558829", "856278", "17652494", "10317769", "10319672", "14001621", "209450652", "715424", "18105754", "10701846", "18414971", "2979783", "11249236", "682575", "18169294", "21720035", "8564041", "17521136", "12182795", "10625626", "18069948", "7893171", "13262315", "30533375", "14440808", "14483336", "6812753", "15399629", "16037955", "15241379", "10659120", "13054181", "13326206", "172131672", "863367", "12007034", "1089413", "10044222", "7784644"], "has_outbound_citations": true, "has_inbound_citations": true, "has_pdf_parse": true, "has_pdf_parsed_abstract": true, "has_pdf_parsed_body_text": true, "has_pdf_parsed_bib_entries": true, "has_pdf_parsed_ref_entries": true, "s2_url": "https://api.sem