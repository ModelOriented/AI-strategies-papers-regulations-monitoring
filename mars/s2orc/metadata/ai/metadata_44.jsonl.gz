90502", "215471", "8788260", "13894550", "122244904", "5986106", "54733116", "4502442", "41121683", "204860486", "16353968", "18644544", "16554469", "18129333", "18775585", "17398825", "31529038", "18095603", "207172859", "17332862", "15493312", "10121016", "203599013", "3171603", "20100", "15791176", "44043502", "14501714", "54833647", "15504638", "15504638", "7496773", "16158459", "3258084", "33655010", "7982044", "16437502", "8357034", "911723", "214743091", "18845884", "5593513", "125208240", "15825183", "14500006", "2380343", "67764234", "59585298", "98175915", "109718214", "5125909", "9823973", "14417331", "57835976", "15739883", "14186961", "18623295", "8262124", "15525663", "67344415", "5571676", "10723208", "17765640", "17318920", "14901434", "389744", "54110472", "4435342", "109634", "9685698", "13986244", "31106092", "5244786", "10179390", "23350405", "2156366", "15114630", "33046408", "4431771", "14488336", "4958225", "9633562", "33791749", "2476376", "18269454", "10560299", "16454925", "382888", "2133158", "39753551", "14211670", "18881989", "210195071", "21596207", "11261917", "5397769", "201235", "13295968", "2854731", "15456354", "6058027", "50306147", "1241717", "5076509", "255854", "9982978", "14658070", "13559630", "9669331", "17793943", "15759462", "10338789", "15236987", "9882273", "8635072", "60569996", "10470970", "15747377", "6547692", "44033248", "13010566", "3226446", "5331420", "15384695", "3723759", "56105769", "37506836", "4964303", "7269216", "14398042", "66943916", "15803019", "59554131", "21144578", "14663899", "891222", "15452607", "56200001", "41422423", "9032999", "14883166", "9511387", "17380760", "18002860", "13084855", "18877374", "209500663", "55115812", "5894296", "60103555", "18954103", "18349579", "61570877", "8357813", "61527979", "4470203", "52089550", "15869105"], "has_outbound_citations": true, "has_inbound_citations": true, "has_pdf_parse": true, "has_pdf_parsed_abstract": false, "has_pdf_parsed_body_text": false, "has_